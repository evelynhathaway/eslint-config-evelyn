{
  "name": "eslint-plugin-evelyn",
  "version": "3.1.0",
  "description": "ESLint plugin for my projects with my preferred code style and bugfixes",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-plugin"
  ],
  "homepage": "https://github.com/evelynhathaway/eslint-plugin-evelyn#readme",
  "bugs": {
    "url": "https://github.com/evelynhathaway/eslint-plugin-evelyn/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evelynhathaway/eslint-plugin-evelyn.git"
  },
  "license": "MIT",
  "author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
  "main": "./lib/index.js",
  "directories": {
    "lib": "/lib",
    "doc": "/docs",
    "test": "/test"
  },
  "files": [
    "/docs",
    "/lib"
  ],
  "scripts": {
    "link": "node ./lib/util/link.js",
    "lint": "eslint ./",
    "pre-commit:lint-staged": "lint-staged",
    "pre-commit:record-changes": "node ./changes/git-hook.js",
    "record-changes": "eslint -c ./changes/record-changes.eslintrc.js ./changes/record-changes.eslintrc.js",
    "test": "run-s test:*",
    "test:configs": "mocha test/configs --recursive",
    "test:final-config-array": "eslint -c ./test/final-config-array.eslintrc.js ./test/final-config-array.eslintrc.js",
    "test:rules": "mocha test/rules --recursive"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s pre-commit:*"
    }
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "./": "eslint --fix"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "eslint": "^7.4.0",
    "eslint-config-rule-tester": "^0.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-unicorn": "^20.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-json": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-mocha": "^7.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "eslint-plugin-vue": "^6.0.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  }
}
