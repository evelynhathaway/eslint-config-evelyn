{
  "name": "eslint-plugin-evelyn",
  "version": "1.0.1",
  "description": "ESLint plugin for my projects with my preferred code style and bugfixes",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-plugin"
  ],
  "homepage": "https://github.com/evelynhathaway/eslint-plugin-evelyn#readme",
  "bugs": {
    "url": "https://github.com/evelynhathaway/eslint-plugin-evelyn/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evelynhathaway/eslint-plugin-evelyn.git"
  },
  "license": "MIT",
  "author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
  "main": "./lib/index.js",
  "directories": {
    "lib": "/lib",
    "doc": "/docs",
    "test": "/test"
  },
  "files": [
    "/docs",
    "/lib"
  ],
  "scripts": {
    "link": "npm link && npm link eslint-plugin-evelyn",
    "lint": "eslint --ext .js ./",
    "record-changes": "eslint -c ./record-changes.eslintrc.js ./record-changes.eslintrc.js && git add ./record-changes.json",
    "test": "run-s test:*",
    "test:configs": "mocha test/configs --recursive",
    "test:final-config-array": "eslint -c ./test/final-config-array.eslintrc.js ./test/final-config-array.eslintrc.js",
    "test:rules": "mocha test/rules --recursive"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run record-changes"
    }
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^10.1.0",
    "chalk": "^3.0.0",
    "eslint": "^6.0.0",
    "eslint-config-rule-tester": "^0.2.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-unicorn": "^9.1.1",
    "husky": "^4.2.3",
    "typescript": "^2.8.0",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.0.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-unicorn": "^9.1.1",
    "typescript": "^2.8.0"
  },
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  }
}
